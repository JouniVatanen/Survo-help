0802|*    $stmt->bindParam(1, $keyword, PDO::PARAM_STR, 100);
0804|*    $isQueryOk = $stmt->execute();
0806|*    $results = array();
0808|*    if ($isQueryOk) {
0809|*      $results = $stmt->fetchAll(PDO::FETCH_COLUMN);
0810|*    } else {
0811|*      trigger_error('Error executing statement.', E_USER_ERROR);
0812|*    }

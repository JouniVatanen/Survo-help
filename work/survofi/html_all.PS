(   4 *C      0   0    0    0    1) s_show
newline
(   5 *B1     0   3    5    1   -1) s_show
newline
(   6 *B2     0   6    2    4   -1) s_show
newline
(   7 *B3     0   1    4    3   -1) s_show
newline
(   8 *B4     0   4    2    5   -1) s_show
newline
(   9 *V      1  -1   -1   -1    0) s_show
newline
(  10 *) s_show

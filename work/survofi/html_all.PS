( 67 *INTERP CUR+2,CUR+4,CUR+5,CUR+7,CUR+1  / DEGREE=2) s_show
newline
( 68 *      YYY) s_show
newline
( 69 *        1) s_show
newline
( 70 *        4) s_show
newline
